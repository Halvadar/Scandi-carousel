{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "rules": {
       "react/prop-types":0,
       "react/display-name":0,
       "import/no-unresolved": [2, { "caseSensitive": true }],
       "no-undef":0,
       "prefer-template":"warn"
       
       
    }
}
